                        <label for="country">Localidad</label>
                        <input type="text" id="lname" name="localidad" minlength= 5 required>
                        <label for="subject">Telefono</label>
                        <input type="text" id="lname" name="telefono" minlength = 10 required>
                    </div>
                    <div class="form-col der">
                        <label for="subject">Mensaje</label>
